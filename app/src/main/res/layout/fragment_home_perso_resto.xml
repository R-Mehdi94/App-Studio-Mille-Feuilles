<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".HomeFragmentPersoResto">


    <ImageView
        android:id="@+id/imgTrans"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:scaleType="@+id/imgVecto"
        android:src="@drawable/sabletransparant">

    </ImageView>


    <ImageView
        android:id="@+id/imgVecto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center">
    </ImageView>

    <TextView
        android:id="@+id/btnRetourMain"
        style="@style/TextPageAccueil"
        android:layout_width="268dp"
        android:layout_height="46dp"
        android:layout_gravity="center|bottom"
        android:layout_marginBottom="35dp"
        android:background="@drawable/roundstyle"
        android:text="@string/retour"
        android:gravity="center"
        android:padding="10dp"
        android:elevation="20dp">

    </TextView>

</FrameLayout>